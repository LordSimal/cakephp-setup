{
	"name": "dereuromark/cakephp-setup",
	"type": "cakephp-plugin",
	"description": "A CakePHP plugin containing lots of useful management tools",
	"keywords": ["cakephp", "plugin", "setup", "management", "backup", "bake", "templates"],
	"homepage": "https://github.com/dereuromark/cakephp-setup",
	"license": "MIT",
	"authors": [
		{
			"name": "Mark Scherer",
			"role": "Author",
			"homepage": "http://www.dereuromark.de"
		}
	],
	"require": {
		"php": ">=5.4",
		"cakephp/cakephp": "~3.1",
		"dereuromark/cakephp-tools": "dev-master"
	},
	"require-dev": {
		"cakephp/migrations": "dev-master"
	},
	"autoload": {
		"psr-4": {
			"Setup\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Setup\\Test\\": "tests",
			"Cake\\Test\\": "vendor/cakephp/cakephp/tests",
			"TestApp\\": "tests/TestApp"
		}
	},
	"support":{
		"source": "https://github.com/dereuromark/cakephp-setup",
		"issues": "https://github.com/dereuromark/cakephp-setup/issues"
	}
}
