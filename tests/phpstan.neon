parameters:
	autoload_files:
		- bootstrap.php
		- shim.php
	ignoreErrors:
		- '#Call to an undefined method .+FlashComponent::warning\(\)#'
		- '#Parameter \#1 \$items of class .+Collection constructor expects iterable, .+StatementInterface given.#'
